{
  "name": "proofofexistence",
  "version": "1.12.2",
  "description": "Web application to prove the existence of files using the bitcoin blockchain.",
  "main": "lib/server.js",
  "scripts": {
    "build": "npm run build:modernizr && ./scripts/vendor.sh",
    "build:modernizr": "modernizr --config modernizr-config.json --dest public/vendor/modernizr.js",
    "clean": "rm -Rf public/vendor",
    "release": "release-it",
    "server": "nodemon server/app.js",
    "sweep": "node lib/sweep.js",
    "test": "mocha --timeout 10000 && standard 'lib/**/*.js' 'config/*.js'",
    "react": "webpack",
    "start": "concurrently \"npm run react\" \"npm run server\"",
    "watch": "nodemon --watch lib --watch config -e js,hbs,yaml,json --exec npm start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/poexio/proofofexistence.git"
  },
  "author": "PoEx Co., Ltd",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@poexio/light-bootstrap-dashboard": "^2.0.2",
    "animate.css": "^3.5.2",
    "axios": "^0.18.0",
    "bitcoin-convert": "^1.0.4",
    "bitcore-lib": "^0.15.0",
    "bootstrap": "^3.3.7",
    "bootstrap-notify": "^3.1.3",
    "browser-request": "^0.3.3",
    "clipboard": "^1.7.1",
    "config": "^1.30.0",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.2",
    "hbs": "^4.0.1",
    "jquery-form": "^3.50.0",
    "level": "^2.0.0",
    "lodash": "^4.0.0",
    "modernizr": "^3.5.0",
    "moment": "^2.19.3",
    "morgan": "^1.9.0",
    "nodemailer": "^4.4.0",
    "openpgp": "^1.3.0",
    "paper-dashboard": "^1.1.0",
    "prompt": "^1.0.0",
    "qrcodejs": "^1.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-dropzone": "^4.2.9",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "sprintf-js": "^1.1.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "mocha": "^4.0.1",
    "nock": "^9.1.5",
    "nodemon": "^1.14.12",
    "release-it": "^7.3.0",
    "standard": "^10.0.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifycss": "0.0.27",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.4",
    "webpack-sass-loaders": "^1.0.0",
    "webpack-stream": "^4.0.0"
